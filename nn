#include<bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    scanf("%d",&n);
    while(n--)
    {
        int n2;scanf("%d",&n2);
        int g=0,r=0,b=0;

        for(int i=0;i<n2;i++)
        {
            char s,s2;cin >> s >> s2;
            if(s=='B' && s2=='G')
                b++;
            else if(s=='B' && s2=='R')
                b+=2;
            else if(s=='R' && s2=='G')
                r+=2;
            else if(s=='R' && s2=='B')
                r++;
            else if(s=='G' && s2=='B')
                g+=2;
            else if(s=='G' && s2=='R')
                g++;
        }
        if(g==b && b>r || g==r && r>b || r==b && b>g)
            printf("empate\n");
        if(g==b && b==r)
            printf("trempate\n");
        else if(g>b && r<=b)
            printf("green\n");
        else if(b>g && r<=g)
            printf("blue\n");
        else if(r>g && g<=b)
            printf("red\n");
    }

    return 0;
}
